{
  "name": "yannisc/verifyemail",
  "description": "Provides means for verification of e-mail addresses.",
  "type": "library",
  "homepage": "https://github.com/masroore/verifyemail",
  "keywords": [
    "php",
    "email",
    "email verification",
    "email validation",
    "email check",
    "smtp",
    "mx"
  ],
  "license": "BSD-2-Clause",
  "require": {
    "php": ">=7.1.3",
    "psr/log": "*",
    "symfony/polyfill-intl-idn": "^1.11",
    "symfony/cache": "^4.2",
    "symfony/polyfill-mbstring": "^1.11"
  },
  "authors": [
    {
      "name": "Masroor Ehsan",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "suggest": {
    "ext-mbstring": "For best performance",
    "ext-intl": "For best performance"
  },
  "autoload": {
    "psr-4": {
      "VerifyEmail\\": "src/"
    }
  }
}
